{
  "name": "todo",
  "version": "0.0.0",
  "author": "Keks",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run lint-watch\" \"next dev\"",
    "start": "concurrently --kill-others \"npm run server\" \"next start\"",
    "build": "next build",
    "build-start": "concurrently --kill-others \"npm run server\" \"npm run build\"",
    "lint": "next lint",
    "lint-watch": "nodemon --watch . --exec npm run lint",
    "server": "json-server-auth --watch ./api/database/database.json --port 5000"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "base64-to-image": "^1.0.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.10.5",
    "dotenv": "^16.1.4",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "jwt-decode": "^3.1.2",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.13.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "nodemon": "^2.0.22"
  },
  "main": "next.config.js"
}
